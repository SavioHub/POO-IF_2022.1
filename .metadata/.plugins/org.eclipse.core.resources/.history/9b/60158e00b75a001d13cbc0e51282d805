package questao2;
import java.util.Scanner;

public class Lapis extends MaterialEscolar {
	Scanner scan = new Scanner(System.in);

	public Lapis() {
		setQtd(1);
		setCor("azul");
		setPreco(2.00);
		setAltura(8.0f);
		setLargura(0.3f);
	}

	
	public void escreverTexto() {
		texto = scan.nextLine();
	}
}
